  shorttitle = {Outbreak Analytics},
  author = {Polonsky, Jonathan A. and Baidjoe, Amrish and Kamvar, Zhian N. and Cori, Anne and Durski, Kara and Edmunds, W. John and Eggo, Rosalind M. and Funk, Sebastian and Kaiser, Laurent and Keating, Patrick and {de Waroux}, Olivier le Polain and Marks, Michael and Moraga, Paula and Morgan, Oliver and Nouvellet, Pierre and Ratnayake, Ruwan and Roberts, Chrissy H. and Whitworth, Jimmy and Jombart, Thibaut},
