  file = {C\:\\Users\\lissa102\\Zotero\\storage\\3DIKPGM2\\Schäfer et al_2017_Emotion Regulation Strategies in Depressive and Anxiety Symptoms in Youth.pdf;C\:\\Users\\lissa102\\Zotero\\storage\\HMZMQEK2\\10.html},
  journal = {Journal of Youth and Adolescence},
